<Window x:Class="ServerGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="430" Width="707">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="172*"/>
            <ColumnDefinition Width="527*"/>
        </Grid.ColumnDefinitions>
        <ListBox Margin="0,79,0,0" Name="lbFileList" SelectionChanged="lbFileList_SelectionChanged"/>
        <Label Content="lFileList" HorizontalAlignment="Left" Height="29" VerticalAlignment="Top" Width="122" Name="lFileList" Margin="0,50,0,0"/>
        <DatePicker HorizontalAlignment="Left" VerticalAlignment="Top" Width="128" Name="dpDatePicker" SelectedDateChanged="dpDatePicker_SelectedDateChanged"/>
        <TextBox ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Auto" Grid.Column="1" HorizontalAlignment="Left" Margin="0,26,0,0" TextWrapping="Wrap" Text="TextBox" Width="527" Name="tbFilePreview" IsReadOnly="True"/>
        <Label Content="lFilePreview" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Name="lFilePreview"/>
        <ComboBox HorizontalAlignment="Left" Height="26" Margin="38,24,0,0" VerticalAlignment="Top" Width="124" Name="cbIPList" SelectionChanged="cbIPList_SelectionChanged"/>
        <Label Content="lIP:" HorizontalAlignment="Left" Margin="0,24,0,0" VerticalAlignment="Top" Name="lIP"/>
        <Ellipse Fill="Yellow" StrokeThickness="1" Stroke="Black" Grid.Column="1" Margin="495,0,0,367" Name="circleNotifier"/>
    </Grid>
</Window>
